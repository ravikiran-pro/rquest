{
  "name": "vite-react-express-boilerplate",
  "version": "1.0.4",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon src/server.js",
    "server:build": "babel --verbose --out-dir dist --only 'src/server.js,src/api/config,src/api/models,src/api/routes,src/api/auth' src",
    "client:build": "vite build",
    "build": "npm run client:build && npm run server:build",
    "serve": "vite preview",
    "format": "prettier . --write",
    "test": ".",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^34.0.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.7",
    "@vitejs/plugin-react": "^1.0.2",
    "antd": "^5.8.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "firebase": "^10.7.2",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.11.0",
    "moment": "^2.29.4",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.45.4",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^5.3.0",
    "react-scroll-to-bottom": "^4.2.0",
    "react-select": "^5.8.0",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "zustand": "^4.4.7"
  }
}
